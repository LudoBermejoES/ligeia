{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "distDir": "../",
    "devPath": "../",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Ligeia",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.ligeia.dev"
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Ligeia - Ambient Soundscape Mixer",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ]
  }
}